{
	"schemaVersion": 1,
	"id": "radishmod",
	"version": "${version}",
	"name": "Radish Mod",
	"description": "This mod adds radishes, radish entities, and other future features to Minecraft!",
	"authors": [
		"Ragsss",
		"Absadoodle",
		"Radishologist"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
